// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Cliente {
  id            Int        @id @default(autoincrement())
  cnpj          String     @unique
  razaoSocial   String
  nomeDoContato String
  telefone      String
  enderecos     Endereco[]
}

model Endereco {
  id          Int      @id @default(autoincrement())
  logradouro  String
  numero      String
  complemento String
  bairro      String
  cidade      String
  estado      String
  cep         String
  latitude    String
  longitude   String
  Cliente     Cliente? @relation(fields: [clienteId], references: [id])
  clienteId   Int?
}
